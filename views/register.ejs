<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="fonts/icomoon/style.css">

    <link rel="stylesheet" href="css/owl.carousel.min.css">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    
    <!-- Style -->
    <link rel="stylesheet" href="..css/style.css">

    <title>Register</title>
  </head>
  <body>
  

  <div class="d-md-flex half">
    <div class="bg" style="background-image: url('images/bg_1.jpg');"></div>
    <div class="contents">

      <div class="container">
        <div class="row align-items-center justify-content-center">
          <div class="col-md-12">
            <div class="form-block mx-auto">
              <div class="text-center mb-3">
              <h3><strong>Create account</strong></h3>
              <!-- <p class="mb-4">Lorem ipsum dolor sit amet elit. Sapiente sit aut eos consectetur adipisicing.</p> -->
              </div>
              <form action="#" method="post">
                <div class="form-group first form-group-padding-bottom form-group-padding-bottom">
                  <label for="email">Email</label>
                  <input name="email" type="text" class="form-control form-control-signup" placeholder="Email" id="email">
                </div>
                <div class="form-group first form-group-padding-bottom">
                    <label for="">Fullname</label>
                    <input name="fullname" type="text" class="form-control form-control-signup" placeholder="Fullname" id="fullname">
                  </div>            
                <div class="form-group first form-group-padding-bottom">
                    <label for="">Phone</label>
                    <input name="phone" type="text" class="form-control form-control-signup" placeholder="Phone Number" id="phone">
                  </div>
                  <div class="form-group first form-group-padding-bottom">
                    <label for="">Address</label>
                    <input name="address" type="text" class="form-control form-control-signup" placeholder="Addresss" id="address">
                  </div>
                  <div class="form-group first form-group-padding-bottom">
                    <label for="">Birthday</label>
                    <input name="birthday" type="date" class="form-control form-control-signup" placeholder="Birthday" id="brithday">
                  </div>
                  <div class="form-group last mb-3">
                    <label for="">Password</label>
                    <input name="password" type="password" class="form-control form-control-signup" placeholder="Your Password" id="password">
                  </div>
                  <div class="form-group first form-group-padding-bottom">
                    <label for="">Repassword</label>
                    <input name="repassword" type="text" class="form-control form-control-signup" placeholder="Repassword" id="repassword">
                  </div>

                <input type="submit" value="Login" class="btn btn-block btn-primary btn-login">
                <span class=" d-sm-flex mt-3 justify-content-center">Already have an account? <a class="ms-2" href="./login">Login</a></span>

              </form>
            </div>
          </div>
        </div>
      </div>
    </div>

    
  </div>
    
    

    <script src="js/jquery-3.3.1.min.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/main.js"></script>
  </body>
</html>